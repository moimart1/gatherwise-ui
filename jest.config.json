{
  "verbose": true,
  "testRegex": ".*\\.test\\.js[x]?$",
  "testPathIgnorePatterns": ["/node_modules/"],
  "roots": ["<rootDir>/src/"],
  "testEnvironment": "jsdom",
  "transform": {
    "\\.(js|jsx)$": "babel-jest",
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/test-helper/fileTransformer.js"
  }
}
