{
  "name": "gatherwise-ui",
  "version": "1.0.0",
  "description": "UI for Gatherwise App",
  "type": "module",
  "keywords": [],
  "scripts": {
    "dev": "VITE_API_URL=https://localhost:3000/api vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "NODE_ENV=test vitest run --coverage.all --reporter=default --coverage.reportsDirectory ${TEST_RESULTS_COVERAGE_REPORT_DIR:-test/results/coverage}",
    "test:html": "npm run test -- --coverage.enabled --coverage.reporter=html --reporter=html && echo \"Coverage available here: ${TEST_RESULTS_COVERAGE_REPORT_DIR:-test/results/coverage}/index.html\"",
    "test:ci": "CI=true npm run test -- --coverage.enabled --reporter=junit --reporter=hanging-process --outputFile.junit=${TEST_RESULTS_DIR:-test/results}/junit-tests.xml",
    "lint": "eslint --ext .js,.jsx src/",
    "lint:ci": "npm run lint -- --output-file ${TEST_RESULTS_LINT_REPORT_PATH:-test/results/eslint-report.xml} --format checkstyle",
    "lint:html": "npm run lint -- --output-file ${TEST_RESULTS_LINT_REPORT_PATH:-test/results/eslint-report.html} --format html",
    "audit:ci": "npm audit --json --omit=dev > ${TEST_RESULTS_AUDIT_REPORT_JSON_PATH:-test/results/npm-audit-report.json}"
  },
  "dependencies": {
    "@chakra-ui/react": "2.2.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@heroicons/react": "^2.0.12",
    "@react-keycloak/web": "^3.4.0",
    "axios": "1.1.3",
    "chakra-react-select": "^4.4.0",
    "dayjs": "^1.11.6",
    "debounce": "^1.2.1",
    "framer-motion": "^7.6.2",
    "react-avatar": "^5.0.3",
    "react-hook-form": "^7.38.0",
    "react-router-dom": "^6.4.2",
    "@fortawesome/pro-regular-svg-icons": "^6.5.2",
    "@fortawesome/pro-solid-svg-icons": "^6.5.2",
    "@headlessui/react": "^2.0.3",
    "@gatherwise/common-frontend-libs": "file:common",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@nabla/vite-plugin-eslint": "^2.0.4",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "axios": "^1.6.8",
    "axios-mock-adapter": "^1.22.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-vitest": "^0.5.4",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.3",
    "vite": "^5.2.11",
    "vite-plugin-webfont-dl": "^3.9.4",
    "vitest": "^1.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}